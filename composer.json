{
    "name": "sovic/common",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sovic\\Common\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
